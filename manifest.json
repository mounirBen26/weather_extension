{
    "manifest_version": 3,
    "name": "Weather Extension",
    "description": "Weather Extension that show current Weather",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./icons/cloudy16.png",
            "48": "./icons/cloudy48.png",
            "128":"./icons/cloudy128.png"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
      }
}